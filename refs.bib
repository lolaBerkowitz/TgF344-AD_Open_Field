	urldate = {2019-01-01},
	journal = {Experimental Brain Research},
	author = {Curthoys, I. S.},
	month = jul,
	year = {1982},
	keywords = {Rat, Guinea pig, Primary afferent neuron, Semicircular canal, Vestibular},
	pages = {286--294}
}

