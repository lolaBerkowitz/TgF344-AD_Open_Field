@article{olson_microglia_2004,
	title = {Microglia {Initiate} {Central} {Nervous} {System} {Innate} and {Adaptive} {Immune} {Responses} through {Multiple} {TLRs}},
	volume = {173},
	copyright = {Copyright © 2004 by The American Association of Immunologists},
	issn = {0022-1767, 1550-6606},
	url = {http://www.jimmunol.org/content/173/6/3916},
	doi = {10.4049/jimmunol.173.6.3916},
	abstract = {Microglia are the resident macrophage-like population in the CNS. Microglia remain quiescent until injury or infection activates the cells to perform effector inflammatory and APC functions. Our previous studies have shown that microglia infected with a neurotropic strain of Theiler’s murine encephalomyelitis virus secreted innate immune cytokines and up-regulated costimulatory molecules and MHC class II, enabling the cells to present viral and myelin Ags to CD4+ T cells. Recently, TLRs have been shown to recognize pathogen-associated molecular patterns and initiate innate immune responses upon interaction with infectious agents. We examined TLR expression on brain microglia and their functional responses upon stimulation with various TLR agonists. We report that mouse microglia express mRNA for all of the recently identified TLRs, TLR1–9, used for recognition of bacterial and viral molecular patterns. Furthermore, stimulation of quiescent microglia with various TLR agonists, including LPS (TLR4), peptidoglycan (TLR2), polyinosinic-polycytidylic acid (TLR3), CpG DNA (TLR9), and infection with viable Theiler’s murine encephalomyelitis virus, activated the cells to up-regulate unique patterns of innate and effector immune cytokines and chemokines at the mRNA and protein levels. In addition, TLR stimulation activated up-regulation of MHC class II and costimulatory molecules, enabling the microglia to efficiently present myelin Ags to CD4+ T cells. Thus, microglia appear to be a unique and important component of both the innate and adaptive immune response, providing the CNS with a means to rapidly and efficiently respond to a wide variety of pathogens.},
	language = {en},
	number = {6},
	urldate = {2017-12-06},
	journal = {The Journal of Immunology},
	author = {Olson, Julie K. and Miller, Stephen D.},
	month = sep,
	year = {2004},
	pmid = {15356140},
	pages = {3916--3924},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\APP7CG4Q\\Olson and Miller - 2004 - Microglia Initiate Central Nervous System Innate a.pdf:application/pdf;Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\2UC9H3V3\\3916.html:text/html}
}

@article{hacquard_interplay_2017,
	title = {Interplay {Between} {Innate} {Immunity} and the {Plant} {Microbiota}},
	volume = {55},
	url = {https://doi.org/10.1146/annurev-phyto-080516-035623},
	doi = {10.1146/annurev-phyto-080516-035623},
	abstract = {The innate immune system of plants recognizes microbial pathogens and terminates their growth. However, recent findings suggest that at least one layer of this system is also engaged in cooperative plant-microbe interactions and influences host colonization by beneficial microbial communities. This immune layer involves sensing of microbe-associated molecular patterns (MAMPs) by pattern recognition receptors (PRRs) that initiate quantitative immune responses to control host-microbial load, whereas diversification of MAMPs and PRRs emerges as a mechanism that locally sculpts microbial assemblages in plant populations. This suggests a more complex microbial management role of the innate immune system for controlled accommodation of beneficial microbes and in pathogen elimination. The finding that similar molecular strategies are deployed by symbionts and pathogens to dampen immune responses is consistent with this hypothesis but implies different selective pressures on the immune system due to contrasting outcomes on plant fitness. The reciprocal interplay between microbiota and the immune system likely plays a critical role in shaping beneficial plant-microbiota combinations and maintaining microbial homeostasis.},
	number = {1},
	urldate = {2017-12-06},
	journal = {Annual Review of Phytopathology},
	author = {Hacquard, Stéphane and Spaepen, Stijn and Garrido-Oter, Ruben and Schulze-Lefert, Paul},
	year = {2017},
	pmid = {28645232},
	pages = {565--589},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\HXPAGWSJ\\Hacquard et al. - 2017 - Interplay Between Innate Immunity and the Plant Mi.pdf:application/pdf}
}

@article{cattaneo_association_2017,
	title = {Association of brain amyloidosis with pro-inflammatory gut bacterial taxa and peripheral inflammation markers in cognitively impaired elderly},
	volume = {49},
	issn = {0197-4580, 1558-1497},
	url = {http://www.neurobiologyofaging.org/article/S0197-4580(16)30197-X/abstract},
	doi = {10.1016/j.neurobiolaging.2016.08.019},
	abstract = {{\textless}h2{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}p{\textgreater}The pathway leading from amyloid-β deposition to cognitive impairment is believed to be a cornerstone of the pathogenesis of Alzheimer's disease (AD). However, what drives amyloid buildup in sporadic nongenetic cases of AD is still unknown. AD brains feature an inflammatory reaction around amyloid plaques, and a specific subset of the gut microbiota (GMB) may promote brain inflammation. We investigated the possible role of the GMB in AD pathogenesis by studying the association of brain amyloidosis with (1) GMB taxa with pro- and anti-inflammatory activity; and (2) peripheral inflammation in cognitively impaired patients. We measured the stool abundance of selected bacterial GMB taxa (\textit{Escherichia/Shigella}, \textit{Pseudomonas aeruginosa}, \textit{Eubacterium rectale}, \textit{Eubacterium hallii}, \textit{Faecalibacterium prausnitzii}, and \textit{Bacteroides fragilis}) and the blood expression levels of cytokines (pro-inflammatory cytokines: CXCL2, CXCL10, interleukin [IL]-1β, IL-6, IL-18, IL-8, inflammasome complex (NLRP3), tumor necrosis factor-alpha [TNF-α]; anti-inflammatory cytokines: IL-4, IL-10, IL-13) in cognitively impaired patients with (\textit{n} = 40, Amy+) and with no brain amyloidosis (\textit{n} = 33, Amy−) and also in a group of controls (\textit{n} = 10, no brain amyloidosis and no cognitive impairment). Amy+ patients showed higher levels of pro-inflammatory cytokines (IL-6, CXCL2, NLRP3, and IL-1β) compared with both controls and with Amy− patients. A reduction of the anti-inflammatory cytokine IL-10 was observed in Amy+ versus Amy−. Amy+ showed lower abundance of \textit{E. rectale} and higher abundance of \textit{Escherichia/Shigella} compared with both healthy controls (fold change, FC = −9.6, \textit{p} {\textless} 0.001 and FC = +12.8, \textit{p} {\textless} 0.001, respectively) and to Amy− (FC = −7.7, \textit{p} {\textless} 0.001 and FC = +7.4, \textit{p} = 0.003). A positive correlation was observed between pro-inflammatory cytokines IL-1β, NLRP3, and CXCL2 with abundance of the inflammatory bacteria taxon \textit{Escherichia/Shigella} (rho = 0.60, \textit{p} {\textless} 0.001; rho = 0.57, \textit{p} {\textless} 0.001; and rho = 0.30, \textit{p} = 0.007, respectively) and a negative correlation with the anti-inflammatory \textit{E. rectale} (rho = −0.48, \textit{p} {\textless} 0.001; rho = −0.25, \textit{p} = 0.024; rho = −0.49, \textit{p} {\textless} 0.001). Our data indicate that an increase in the abundance of a pro-inflammatory GMB taxon, \textit{Escherichia/Shigella}, and a reduction in the abundance of an anti-inflammatory taxon, \textit{E. rectale}, are possibly associated with a peripheral inflammatory state in patients with cognitive impairment and brain amyloidosis. A possible causal relation between GMB-related inflammation and amyloidosis deserves further investigation.{\textless}/p{\textgreater}},
	language = {English},
	urldate = {2017-12-06},
	journal = {Neurobiology of Aging},
	author = {Cattaneo, Annamaria and Cattane, Nadia and Galluzzi, Samantha and Provasi, Stefania and Lopizzo, Nicola and Festari, Cristina and Ferrari, Clarissa and Guerra, Ugo Paolo and Paghera, Barbara and Muscio, Cristina and Bianchetti, Angelo and Volta, Giorgio Dalla and Turla, Marinella and Cotelli, Maria Sofia and Gennuso, Michele and Prelle, Alessandro and Zanetti, Orazio and Lussignoli, Giulia and Mirabile, Dario and Bellandi, Daniele and Gentile, Simona and Belotti, Gloria and Villani, Daniele and Harach, Taoufiq and Bolmont, Tristan and Padovani, Alessandro and Boccardi, Marina and Frisoni, Giovanni B.},
	month = jan,
	year = {2017},
	pmid = {27776263, 27776263},
	pages = {60--68},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\WUXG9CG7\\Cattaneo et al. - 2017 - Association of brain amyloidosis with pro-inflamma.pdf:application/pdf}
}

@article{sternberg_neural_2006,
	title = {Neural regulation of innate immunity: a coordinated nonspecific host                     response to pathogens},
	volume = {6},
	issn = {1474-1733},
	shorttitle = {Neural regulation of innate immunity},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC1783839/},
	doi = {10.1038/nri1810},
	abstract = {The central nervous system (CNS) regulates innate immune responses
                    through hormonal and neuronal routes. The neuroendocrine stress response and the
                    sympathetic and parasympathetic nervous systems generally inhibit innate immune
                    responses at systemic and regional levels, whereas the peripheral nervous system
                    tends to amplify local innate immune responses. These systems work together to
                    first activate and amplify local inflammatory responses that contain or
                    eliminate invading pathogens, and subsequently to terminate inflammation and
                    restore host homeostasis. Here, I review these regulatory mechanisms and discuss
                    the evidence indicating that the CNS can be considered as integral to
                    acute-phase inflammatory responses to pathogens as the innate immune system.},
	number = {4},
	urldate = {2017-12-06},
