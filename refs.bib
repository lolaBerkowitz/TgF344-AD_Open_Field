	file = {ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\P2TSL477\\Yu et al. - 2006 - Backward Shift of Head Direction Tuning Curves of .pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\VCXSK9N4\\S0896627306008002.html:text/html}
