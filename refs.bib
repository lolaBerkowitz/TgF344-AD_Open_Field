	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\6KJBWN24\\Vogt et al. - 2017 - Gut microbiome alterations in Alzheimer’s disease.pdf:application/pdf;Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\C7SV4ZFT\\s41598-017-13601-y.html:text/html}
