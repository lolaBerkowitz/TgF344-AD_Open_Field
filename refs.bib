	keywords = {Female, Humans, Middle Aged, Adolescent, Adult, Space Perception, Orientation, Cognition, Young Adult, Choice Behavior, Social Environment, Recognition (Psychology), Aptitude, Cities, Individuality, Students, Transportation, Walking},
	pages = {309--326}
}

@article{prestopnik_relations_2000,
	title = {{THE} {RELATIONS} {AMONG} {WAYFINDING} {STRATEGY} {USE}, {SENSE} {OF} {DIRECTION}, {SEX}, {FAMILIARITY}, {AND} {WAYFINDING} {ABILITY}},
