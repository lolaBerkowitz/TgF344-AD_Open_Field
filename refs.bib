	pmid = {28404803},
	pmcid = {PMC5419982},
	keywords = {Female, Aged, Alzheimer Disease, Brain, Brain Mapping, Follow-Up Studies, Humans, Magnetic Resonance Imaging, Male, Prognosis, Biomarkers, Neuropsychological Tests, Prodromal Symptoms, tau Proteins, Aniline Compounds, Apolipoprotein E4, Ethylene Glycols, Fluorodeoxyglucose F18, Irritable Mood, Positron-Emission Tomography, Radiopharmaceuticals, Sleep},
