	issn = {0166-4328},
	url = {http://www.sciencedirect.com/science/article/pii/S0166432896890802},
	doi = {10.1016/S0166-4328(96)89080-2},
	abstract = {Groups of rats received cytotoxic lesions centred in either the anterior medial thalamic nucleus (AM), the anterior ventral and anterior dorsal thalamic nuclei (AV/AD), or all three nuclei combined (ANT.T). These lesions were made by injecting N-methyl-d-aspartate acid (NMDA). These rats, and a group of surgical controls (SHAM), were trained on a rewarded forced-alternation task in a T-maze. While the selective AM and AV/AD lesions prduced an initial acquisition impairment, only the animals with combined lesions (ANT.T) showed a persistent deficit throughout the 16 acquisition sessions. Subsequent testing with a cross-maze confirmed that the SHAM, AV/AD, and AM groups were able to use allocentric cues, while the ANT.T group were impaired. In contrast none of the three anterior thalamic groups were impaired on a subsequent egocentric discrimination and reversal task run in the same apparatus. A final test using the eight arm radial-maze, revealed marked deficits in the ANT.T group as well as milder deficits in the AV/AD group. The results from these experiments help to confirm the importance of the anterior thalamic nuclei for allocentric tasks, but suggest that no region is pre-eminently important. The findings also help to account for other studies which have reported that anterior thalamic lesions have seemingly mild effects on tests of spatial memory.},
	number = {1},
	urldate = {2018-05-20},
	journal = {Behavioural Brain Research},
	author = {Aggleton, J. P. and Hunt, P. R. and Nagle, S. and Neave, N.},
	month = nov,
	year = {1996},
	keywords = {Spatial memory, Thalamus, Anterior thalamus, Rat, Mamillary body, Spatial alternation},
	pages = {189--198},
	file = {ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\CWHYFAFR\\Aggleton et al. - 1996 - The effects of selective lesions within the anteri.pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\IBS2SJ4T\\S0166432896890802.html:text/html}
}

