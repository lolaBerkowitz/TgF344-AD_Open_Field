	journal = {Behavioral Neuroscience},
	author = {Smith, Kyle S. and Bucci, David J. and Luikart, Bryan W. and Mahler, Stephen V.},
	month = apr,
	year = {2016},
	keywords = {Memory, Learning, addiction, Behavioral Neuroscience, chemogenetics, Chemoreceptors, Drugs, Genetics, learning and memory, motivation, reward},
	pages = {137--155},
	file = {EBSCO Full Text:d\:\\Users\\BClarkLab\\Zotero\\storage\\KYGWY6UD\\Smith et al. - 2016 - DREADDS Use and application in behavioral neurosc.pdf:application/pdf}
}

