@article{muir_disruption_2009,
	title = {{DISRUPTION} {OF} {THE} {HEAD} {DIRECTION} {CELL} {SIGNAL} {FOLLOWING} {OCCLUSION} {OF} {THE} {SEMICIRCULAR} {CANALS} {IN} {THE} {FREELY}-{MOVING} {CHINCHILLA}},
