	pages = {1003--1012},
	file = {Passecker et al. - 2018 - Influences of photic stress on postsubicular head-.pdf:d\:\\Users\\BClarkLab\\Zotero\\storage\\ZH56RPEX\\Passecker et al. - 2018 - Influences of photic stress on postsubicular head-.pdf:application/pdf}
}

