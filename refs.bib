	author = {Lai, Chun-Hong and Ma, Chun-Wai and Lai, Suk-King and Han, Lei and Wong, Hoi-Man and Yeung, Kelvin Wai-Kwok and Shum, Daisy Kwok-Yan and Chan, Ying-Shing},
