	abstract = {Based on head direction experiments in rats, the existence of localized bumps of thalamic activity has been proposed. We computationally demonstrate the existence of a novel class of localized bump solutions in a two-layer conductance-based thalamic network and analyze the mechanisms behind these stable patterns. In contrast to previous models of bump activity, here inhibition plays a crucial role in initially spreading neuronal ﬁring and in subsequently sustaining it. In our model, we incorporate local strong, fast GABAA inhibition and diffuse weak, slow GABAB inhibition, based on previous biophysical experiments. These forms of inhibition contribute in different, yet complementary, ways to the observed pattern formation.},
	language = {en},
	author = {Rubin, Jonathan and Terman, David and Chow, Carson},
	pages = {19},
	file = {Rubin et al. - Localized Bumps of Activity Sustained by Inhibitio.pdf:d\:\\Users\\BClarkLab\\Zotero\\storage\\YLV6WPXA\\Rubin et al. - Localized Bumps of Activity Sustained by Inhibitio.pdf:application/pdf}
}

