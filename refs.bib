	journal = {Journal of Comparative Neurology},
	author = {Oda, Satoko and Funato, Hiromasa and Sato, Fumi and Adachi‐Akahane, Satomi and Ito, Masanori and Takase, Kenkichi and Kuroda, Masaru},
	month = jun,
	year = {2014},
	keywords = {anterodorsal thalamic nucleus, anteroventral thalamic nucleus, laterodorsal thalamic nucleus, reticular thalamic nucleus, retrosplenial granular cortex},
	pages = {2089--2106},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\C6GJVZGQ\\Oda et al. - 2014 - A subset of thalamocortical projections to the ret.pdf:application/pdf;Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\4CRS86KE\\cne.html:text/html}
}

@article{harland_anterior_2014,
	title = {Anterior thalamic lesions reduce spine density in both hippocampal {CA1} and retrosplenial cortex, but enrichment rescues {CA1} spines only: {ATN} {LESIONS}, {ENRICHMENT}, {CA1} {AND} {RETROSPLENIAL} {SPINE} {DENSITY}},
	volume = {24},
	issn = {10509631},
