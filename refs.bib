	pmcid = {PMC5092189},
	file = {PubMed Central Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\RW5KLJ3B\\Chan et al. - 2016 - The 4 Mountains Test A Short Test of Spatial Memo.pdf:application/pdf}
}

@article{lawton_strategies_1996,
	title = {{STRATEGIES} {FOR} {INDOOR} {WAYFINDING}: {THE} {ROLE} {OF} {ORIENTATION}},
