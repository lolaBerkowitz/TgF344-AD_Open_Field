	file = {Shenoda - 2017 - An Overview of the Mechanisms of Abnormal GABAergi.pdf:d\:\\Users\\BClarkLab\\Zotero\\storage\\8IK4P8E4\\Shenoda - 2017 - An Overview of the Mechanisms of Abnormal GABAergi.pdf:application/pdf}
}

@article{freund_interneurons_1998,
	title = {Interneurons of the hippocampus},
