@article{brandon_segregation_2013,
	title = {Segregation of cortical head direction cell assemblies on alternating theta cycles},
	volume = {16},
	issn = {1097-6256},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3703458/},
	doi = {10.1038/nn.3383},
	abstract = {High-level cortical systems for spatial navigation, including entorhinal grid cells, critically depend on input from the head direction system. We examined spiking rhythms and modes of synchrony between neurons participating in head direction networks for evidence of internal processing, independent of direct sensory drive, which may be important for grid cell function. We demonstrate that head direction networks of rats are segregated into at least two populations of neurons firing on alternate theta cycles (theta cycle skipping) with fixed synchronous or anti-synchronous relationships. Pairs of anti-synchronous theta cycle skipping neurons exhibited larger differences in head direction tuning with a minimum difference of 40 degrees of head direction. Septal inactivation preserved the head direction signal but eliminated theta cycle skipping of head direction cells and grid cell spatial periodicity. We propose that internal mechanisms underlying cycle skipping in head direction networks may be critical for downstream spatial computation by grid cells.},
	number = {6},
	urldate = {2018-12-28},
	journal = {Nature neuroscience},
	author = {Brandon, Mark P. and Bogaard, Andrew R. and Schultheiss, Nathan W. and Hasselmo, Michael E.},
	month = jun,
	year = {2013},
