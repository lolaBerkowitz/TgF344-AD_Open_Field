@article{ohara_intrinsic_2018,
	title = {Intrinsic {Projections} of {Layer} {Vb} {Neurons} to {Layers} {Va}, {III}, and {II} in the {Lateral} and {Medial} {Entorhinal} {Cortex} of the {Rat}},
