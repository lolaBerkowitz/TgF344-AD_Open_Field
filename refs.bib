	file = {Accepted Version:d\:\\Users\\BClarkLab\\Zotero\\storage\\8B57JXMJ\\Gold et al. - 2009 - High-Amplitude Positive Spikes Recorded Extracellu.pdf:application/pdf}
}

@article{cornford_dendritic_2019,
	title = {Dendritic {NMDA} receptors in parvalbumin neurons enable strong and stable neuronal assemblies},
