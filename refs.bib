	keywords = {Animals, Female, Rats, Spatial Behavior, Brain Mapping, Hippocampus, Maze Learning, Space Perception, Cues, Orientation, Time Factors, Head, Thalamus, Rats, Long-Evans, rats, Association Learning, Auditory Perception, discharge \& preferred firing direction of head cells in anterior dorsal thalamus \& postsubiculum, Electrical Activity, Environmental Effects, Evoked Potentials, Neural Analyzers, nonvisual or unfamiliar environmental cues, Smell, Spatial Orientation (Perception), Stimulus Novelty, Stimulus Presentation Methods, Synaptic Transmission},
	pages = {749--761},
