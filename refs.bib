@article{goodridge_preferential_1995,
	title = {Preferential {Use} of the {Landmark} {Navigational} {System} by {Head} {Direction} {Cells} in {Rats}},
	abstract = {Previous studies have identified a population of cells recorded in the postsubiculum and the anterior thalamic nucleus (ATN) that discharge as a function of an animal's head direction (HD) in the horizontal plane. The present experiments monitored HD cell activity when rats were confronted with a situation in which directional information from internal sensory sources (e.g., proprioceptive, vestibular, or motor efference copy) conflicted with directional information derived from familiar, external landmarks. Results showed that when a salient, familiar cue was reintroduced to rat's environment into a position that conflicted with the cell's current firing direction, HD cells in both the ATN and the postsubiculum shifted their preferred direction to reflect their originally established orientation with this cue. This finding suggests that sensory inputs onto HD cells from external landmark cues are capable of overriding spatial information developed through internal sensory cues.},
	language = {en},
	author = {Goodridge, Jeremy P and Taube, Jeffrey S},
	month = feb,
	year = {1995},
