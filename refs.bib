	urldate = {2019-09-05},
	journal = {Biological Psychiatry},
	author = {Voorhees, Jaymie R. and Remy, Matthew T. and Cintrón-Pérez, Coral J. and El Rassi, Eli and Khan, Michael Z. and Dutca, Laura M. and Yin, Terry C. and McDaniel, Latisha N. and Williams, Noelle S. and Brat, Daniel J. and Pieper, Andrew A.},
	month = oct,
	year = {2018},
	keywords = {Depression, Alzheimer’s disease, Neurodegeneration, Neurogenesis, Neuroprotection, P7C3, TgF344-AD},
	pages = {488--498},
	file = {ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\BZ7MCBE8\\Voorhees et al. - 2018 - (−)-P7C3-S243 Protects a Rat Model of Alzheimer’s .pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\JAYZ9DQQ\\S0006322317321455.html:text/html}
