	keywords = {Rats, Learning, Velocity, Vision, Eigenvalues, Musculoskeletal system, Optimization, Primates},
	pages = {e166},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\WENELR54\\Franzius et al. - 2007 - Slowness and Sparseness Lead to Place, Head-Direct.pdf:application/pdf;Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\VQTIKL5M\\article.html:text/html}
