	month = apr,
	year = {2017},
	keywords = {elderly, host defense, immunosenescence, inflamm-aging, inflammation, macrophage, microbiome, microbiota},
	pages = {455--466.e4},
	file = {ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\KW8K9BUB\\Thevaranjan et al. - 2017 - Age-Associated Microbial Dysbiosis Promotes Intest.pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\SCQKGTAY\\S1931312817301129.html:text/html}
}

