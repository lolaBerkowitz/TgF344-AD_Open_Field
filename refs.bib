	journal = {Journal of Neuroscience},
	author = {Butler, William N. and Taube, Jeffrey S.},
	month = feb,
	year = {2015},
	pmid = {25673848},
	keywords = {navigation, place cell, vestibular, spatial orientation, neural integrator, oculomotor},
	pages = {2547--2558},
