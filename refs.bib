@article{meister_getting_2016,
	series = {Hippocampal {Interactions} with {Brain} {Networks} that {Influence} {Learning} \& {Memory}},
	title = {Getting directions from the hippocampus: {The} neural connection between looking and memory},
	volume = {134},
	issn = {1074-7427},
	shorttitle = {Getting directions from the hippocampus},
	url = {http://www.sciencedirect.com/science/article/pii/S1074742715002348},
	doi = {10.1016/j.nlm.2015.12.004},
	abstract = {Investigations into the neural basis of memory in human and non-human primates have focused on the hippocampus and associated medial temporal lobe (MTL) structures. However, how memory signals from the hippocampus affect motor actions is unknown. We propose that approaching this question through eye movement, especially by assessing the changes in looking behavior that occur with experience, is a promising method for exposing neural computations within the hippocampus. Here, we review how looking behavior is guided by memory in several ways, some of which have been shown to depend on the hippocampus, and how hippocampal neural signals are modulated by eye movements. Taken together, these findings highlight the need for future research on how MTL structures interact with the oculomotor system. Probing how the hippocampus reflects and impacts motor output during looking behavior renders a practical path to advance our understanding of the hippocampal memory system.},
	urldate = {2018-05-21},
	journal = {Neurobiology of Learning and Memory},
	author = {Meister, Miriam L. R. and Buffalo, Elizabeth A.},
	month = oct,
	year = {2016},
	keywords = {Memory, Hippocampus, Neurons, Eye movements, Free-viewing, Medial temporal lobe, Primate, Saccades},
	pages = {135--144},
	file = {ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\TGQ79KMD\\Meister and Buffalo - 2016 - Getting directions from the hippocampus The neura.pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\ARD4XHPY\\S1074742715002348.html:text/html}
}

@article{laurens_brain_2018,
	title = {The {Brain} {Compass}: {A} {Perspective} on {How} {Self}-{Motion} {Updates} the {Head} {Direction} {Cell} {Attractor}},
	volume = {97},
	issn = {0896-6273},
	shorttitle = {The {Brain} {Compass}},
	url = {https://www.cell.com/neuron/abstract/S0896-6273(17)31165-0},
	doi = {10.1016/j.neuron.2017.12.020},
	abstract = {Head direction cells form an internal compass signaling head azimuth orientation even without visual landmarks. This property is generated by a neuronal ring attractor that is updated using rotation velocity cues. The properties and origin of this velocity drive remain, however, unknown. We propose a quantitative framework whereby this drive represents a multisensory self-motion estimate computed through an internal model that uses sensory prediction errors of vestibular, visual, and somatosensory cues to improve on-line motor drive. We show how restraint-dependent strength of recurrent connections within the attractor can explain differences in head direction cell firing between free foraging and restrained passive rotation. We also summarize recent findings on how gravity influences azimuth coding, indicating that the velocity drive is not purely egocentric. Finally, we show that the internal compass may be three-dimensional and hypothesize that the additional vertical degrees of freedom use global allocentric gravity cues.},
	language = {English},
	number = {2},
	urldate = {2018-05-21},
	journal = {Neuron},
	author = {Laurens, Jean and Angelaki, Dora E.},
	month = jan,
	year = {2018},
	pmid = {29346751},
	keywords = {navigation, attractor network, internal model, vestibular, virtual reality},
	pages = {275--289},
	file = {Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\YE7NFQG2\\S0896-6273(17)31165-0.html:text/html}
}

