	pmid = {15385610},
	keywords = {hippocampus, navigation, spatial, path integration, orientation, maze},
	pages = {8265--8277},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\RC8ZUVW4\\Cacucci et al. - 2004 - Theta-Modulated Place-by-Direction Cells in the Hi.pdf:application/pdf;Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\7ZN8YQVG\\8265.html:text/html}
