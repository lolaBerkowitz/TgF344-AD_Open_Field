	pmcid = {PMC2991559},
	pages = {S204--S217},
	file = {PubMed Central Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\UW6KAL8X\\Koronyo-Hamaoui et al. - 2011 - Identification of Amyloid Plaques in Retinas from .pdf:application/pdf}
