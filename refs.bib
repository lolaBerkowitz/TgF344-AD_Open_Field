	month = feb,
	year = {2017},
	keywords = {Alzheimer’s disease, cognitive deficits, electrophysiology, entorhinal cortex, grid cells, hypoactivity, neurofibrillary tangles, neuronal loss, spatial memory, tau pathology},
	pages = {533--541.e5},
	file = {ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\JDKATAP3\\Fu et al. - 2017 - Tau Pathology Induces Excitatory Neuron Loss, Grid.pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\AZM7YA46\\S0896627316309965.html:text/html}
