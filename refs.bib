	journal = {Frontiers in Molecular Neuroscience},
	author = {Raj, Divya and Yin, Zhuoran and Breur, Marjolein and Doorduin, Janine and Holtman, Inge R. and Olah, Marta and Mantingh-Otter, Ietje J. and Van Dam, Debby and De Deyn, Peter P. and den Dunnen, Wilfred and Eggen, Bart J. L. and Amor, Sandra and Boddeke, Erik},
	year = {2017},
	keywords = {Aging, Alzheimer’s disease, Microglia, Neuroinflammation, white matter},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\IR993L2E\\Raj et al. - 2017 - Increased White Matter Inflammation in Aging- and .pdf:application/pdf}
}

