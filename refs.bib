	journal = {European Journal of Neuroscience},
	author = {Nestor, P. J. and Fryer, T. D. and Ikeda, M. and Hodges, J. R.},
	year = {2003},
	keywords = {human, amnesia, episodic memory, positron emission tomography},
	pages = {2663--2667},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\BXDXDID6\\Nestor et al. - 2003 - Retrosplenial cortex (BA 2930) hypometabolism in .pdf:application/pdf;Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\5CCSVYID\\j.1460-9568.2003.02999.html:text/html}
