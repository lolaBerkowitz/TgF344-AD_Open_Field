	keywords = {Navigation, Head direction cell, Spatial cognition, Vestibular system, Dorsal tegmental nucleus, Mammillary nuclei},
	pages = {138--148},
	file = {1-s2.0-S0006899306010158-main.pdf:d\:\\Users\\BClarkLab\\Zotero\\storage\\4MTFHPM7\\1-s2.0-S0006899306010158-main.pdf:application/pdf;ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\4ARZLSJ9\\Biazoli et al. - 2006 - The supragenual nucleus A putative relay station .pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\L7R488KI\\S0006899306010158.html:text/html}
