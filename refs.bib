@article{yoganarasimha_head_2006,
	title = {Head direction cell representations maintain internal coherence during conflicting proximal and distal cue rotations: {Comparison} with hippocampal place cells},
	volume = {26},
	issn = {0270-6474},
	shorttitle = {Head direction cell representations maintain internal coherence during conflicting proximal and distal cue rotations},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC1388189/},
	doi = {10.1523/JNEUROSCI.3885-05.2006},
	abstract = {Place cells of the hippocampal formation encode a spatial representation of the environment, and the orientation of this representation is apparently governed by the head direction cell system. The representation of a well-explored environment by CA1 place cells can be split when there is conflicting information from salient proximal and distal cues, as some place fields rotate to follow the distal cues while others rotate to follow the proximal cues (). In contrast, the CA3 representation is more coherent than CA1, as the place fields in CA3 tend to rotate in the same direction (). The present study tests whether the head direction cell network produces a split representation or remains coherent under these conditions, by simultaneously recording both CA1 place cell and head direction cells from the thalamus. In agreement with earlier studies, split representations of the environment were observed in ensembles of CA1 place cells in about 75\% of the mismatch sessions, in which some fields followed the counterclockwise rotation of proximal cues and other fields followed the clockwise rotation of distal cues. However, out of 225 recording sessions, there was not a single instance of the head direction cell ensembles revealing a split representation of head direction. Instead, in most of the mismatch sessions the head direction cell tuning curves rotated as an ensemble clockwise (94\%) and in a few sessions rotated counterclockwise (6\%). The findings support the notion that the head direction cells may be part of an attractor network bound more strongly to distal landmarks than proximal landmarks, even under conditions in which the CA1 place representation loses its coherence.},
	number = {2},
	urldate = {2017-12-18},
	journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	author = {Yoganarasimha, D. and Yu, Xintian and Knierim, James J.},
	month = jan,
	year = {2006},
	pmid = {16407560},
	pmcid = {PMC1388189},
	pages = {622--631},
	file = {PubMed Central Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\LY8KNWNF\\Yoganarasimha et al. - 2006 - Head direction cell representations maintain inter.pdf:application/pdf}
}

@article{taube_lesions_1992,
	title = {Lesions of the rat postsubiculum impair performance on spatial tasks},
	volume = {57},
	issn = {0163-1047},
	abstract = {Previous studies have identified a population of neurons in the postsubiculum that discharge as a function of the rat's head direction in the horizontal plane (Taube, Muller, \& Ranck, 1990a). To assess the contribution of these cells in spatial learning, Long-Evans rats were tested in a variety of spatial and nonspatial tasks following bilateral electrolytic or neurotoxic lesions of the postsubiculum. Compared to unlesioned control animals, lesioned animals were impaired on two spatial tasks, a radial eight-arm maze task and a Morris water task, although the performance scores of both lesion groups improved over the course of behavioral testing. In contrast, lesioned animals were unimpaired on two nonspatial tasks, a cued version of the water maze task and a conditioned taste-aversion paradigm. In addition, lesioned animals showed transient hyperactivity in an open-field activity test. These results support the concept that neurons in the postsubiculum are part of a neural network involved in the processing of spatial information.},
	language = {eng},
	number = {2},
	journal = {Behavioral and Neural Biology},
	author = {Taube, J. S. and Kesslak, J. P. and Cotman, C. W.},
	month = mar,
	year = {1992},
	pmid = {1586352},
	keywords = {Animals, Discrimination Learning, Rats, Retention (Psychology), Brain Mapping, Male, Hippocampus, Orientation, Neurons, Motor Activity, Escape Reaction, Mental Recall, Avoidance Learning, Neural Pathways, Fear, Cerebral Cortex, Conditioning, Classical, Social Environment, Taste},
	pages = {131--143}
}

@article{harvey_post-training_2017,
	title = {Post-training {Inactivation} of the {Anterior} {Thalamic} {Nuclei} {Impairs} {Spatial} {Performance} on the {Radial} {Arm} {Maze}},
	volume = {11},
	issn = {1662-4548},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5337504/},
	doi = {10.3389/fnins.2017.00094},
	abstract = {The limbic thalamus, specifically the anterior thalamic nuclei (ATN), contains brain signals including that of head direction cells, which fire as a function of an animal's directional orientation in an environment. Recent work has suggested that this directional orientation information stemming from the ATN contributes to the generation of hippocampal and parahippocampal spatial representations, and may contribute to the establishment of unique spatial representations in radially oriented tasks such as the radial arm maze. While previous studies have shown that ATN lesions can impair spatial working memory performance in the radial maze, little work has been done to investigate spatial reference memory in a discrimination task variant. Further, while previous studies have shown that ATN lesions can impair performance in the radial maze, these studies produced the ATN lesions prior to training. It is therefore unclear whether the ATN lesions disrupted acquisition or retention of radial maze performance. Here, we tested the role of ATN signaling in a previously learned spatial discrimination task on a radial arm maze. Rats were first trained to asymptotic levels in a task in which two maze arms were consistently baited across training. After 24 h, animals received muscimol inactivation of the ATN before a 4 trial probe test. We report impairments in post-inactivation trials, suggesting that signals from the ATN modulate the use of a previously acquired spatial discrimination in the radial-arm maze. The results are discussed in relation to the thalamo-cortical limbic circuits involved in spatial information processing, with an emphasis on the head direction signal.},
	urldate = {2017-12-18},
	journal = {Frontiers in Neuroscience},
	author = {Harvey, Ryan E. and Thompson, Shannon M. and Sanchez, Lilliana M. and Yoder, Ryan M. and Clark, Benjamin J.},
	month = mar,
	year = {2017},
	pmid = {28321178},
	pmcid = {PMC5337504},
	file = {PubMed Central Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\UMKMQK6E\\Harvey et al. - 2017 - Post-training Inactivation of the Anterior Thalami.pdf:application/pdf}
}

@article{derdikman_head-direction_2017,
	title = {Head-{Direction} drift in rat pups is consistent with an angular path-integration process},
	copyright = {© 2017, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution 4.0 International), CC BY 4.0, as described at http://creativecommons.org/licenses/by/4.0/},
	url = {https://www.biorxiv.org/content/early/2017/11/01/212852},
	doi = {10.1101/212852},
	abstract = {The sense of direction is a vital computation, whose neural basis is considered to be carried out by head-direction cells. One way to estimate head-direction is by integrating head angular-velocity over time. However, this process results in error accumulation resembling a random walk, proportional to , which constitutes a mark for a path integration process. In the present study we analyzed previously recorded data to quantify the drift in head-direction cells of rat pups before and after eye-opening. We found that in rat pups before eye-opening the drift propagated as a random walk, while in rats after eye-opening the drift was lower. This suggests that a path-integration process underlies the estimation of head-direction, such that before eye-opening the head-direction system runs in an open-loop manner and accumulates error. After eye-opening, visual-input, such as arena shape, helps to correct errors and thus compute the sense of direction accurately.},
	language = {en},
	urldate = {2017-12-18},
	journal = {bioRxiv},
	author = {Derdikman, Dori and Tocker, Gilad and Borodach, Eli and Bjerknes, Tale L. and Moser, May-Britt and Moser, Edvard I.},
	month = nov,
	year = {2017},
	pages = {212852},
	file = {Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\QY2N68AN\\Derdikman et al. - 2017 - Head-Direction drift in rat pups is consistent wit.pdf:application/pdf;Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\SNGZWSR8\\212852.html:text/html}
}

