	author = {Moffat, Scott D. and Elkins, Wendy and Resnick, Susan M.},
	month = jul,
	year = {2006},
	keywords = {Hippocampus, Alzheimer's disease, Navigation, Spatial memory, fMRI, Cognitive aging, Human, Parahippocampal gyrus, Retro-splenial cortex},
	pages = {965--972},
	file = {ScienceDirect Full Text PDF:d\:\\Users\\BClarkLab\\Zotero\\storage\\MZ8PPHMZ\\Moffat et al. - 2006 - Age differences in the neural systems supporting h.pdf:application/pdf;ScienceDirect Snapshot:d\:\\Users\\BClarkLab\\Zotero\\storage\\XANKSKCC\\S019745800500120X.html:text/html}
