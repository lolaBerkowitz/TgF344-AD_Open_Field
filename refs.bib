	pmid = {26829469},
	keywords = {Animals, Alzheimer Disease, Humans, Alzheimer's disease, Disease Models, Animal, Animal models, Behavioral Research, Non-human primates, Translational Medical Research},
	pages = {112--114}
}

@article{clinton_age-dependent_2007,
	title = {Age-dependent sexual dimorphism in cognition and stress response in the {3xTg}-{AD} mice},
	volume = {28},
	issn = {0969-9961},
	doi = {10.1016/j.nbd.2007.06.013},
	abstract = {We sought to determine if sex impacts the cognitive and neuropathological phenotype of the 3xTg-AD mice. We find that male and female 3xTg-AD mice show comparable impairments on Morris water maze (MWM) and inhibitory avoidance (IA) at 4 months. Shortly thereafter, however, the cognitive performance varies among the sexes, with females performing worse than males. These behavioral differences are not attributable to differences in Abeta or tau levels. The behavioral effect is transient as from 12 months onward, the disparity is no longer apparent. Because females perform worse than males on stressful tasks, we explored their corticosterone responses and find that young female 3xTg-AD mice show markedly heightened corticosterone response after 5 days of MWM training compared to age-matched male 3xTg-AD mice; this difference is no longer apparent in older mice. Thus, the enhanced corticosterone response of the young female mice likely underlies their poorer performance on stressful tasks.},
	language = {eng},
	number = {1},
	journal = {Neurobiology of Disease},
	author = {Clinton, Lani K. and Billings, Lauren M. and Green, Kim N. and Caccamo, Antonella and Ngo, Jerry and Oddo, Salvatore and McGaugh, James L. and LaFerla, Frank M.},
	month = oct,
